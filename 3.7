#include <stdio.h>
#include <stdlib.h>

int main()
{
    int N,M,K,i,S;
    scanf("%d", &N);
    scanf("%d", &M);
    int arr[N];
    for(i=1;i<=N;i++)
    arr[i]=1;
    K=1;
    S=1;
    i=0;
    while (S<=N-3)
    {
        while (K<=M)
        {
             if (i<N) i++; else i=1;
             if (arr[i]==1) K++;
        }
        arr[i]=0;
        S++;
        K=1;
    }
    for(i=1;i<=N;i++)
    {
        if (arr[i]==1)
        printf("%d\n", i);
    }
    return 0;
}
