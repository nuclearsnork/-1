#include <stdio.h>
#include <stdlib.h>

int main()
{
    int S,n,a[n],i,max,k;
    scanf("%d", &n);
    max=0;
    k=0;
    S=10000;
    for(i=1;i<=n;i++)
    {
        scanf("%d", &a[i]);
    }
    while (S>0)
    {
        for(i=1;i<=n;i++)
        {
            if (a[i]>max)
            {
                max=a[i];
                k=i;
            }
        }
        if (max<=S)
        {
            S=S-max;
            printf ("%d", max);
            max=0;
        }
        else
        {
            a[k]=0;
            max=0;
        }
    }
    return 0;
}
