#include <stdio.h>
#include <stdlib.h>

int fact (int n)
{
    int i,f=1;
    for(i=1;i<=n;i++)
    f=f*i;
    return f;
}
int main ()
{
    int n,c,i;
    scanf("%d", &n);
    int array_c[n/2];
    if (n%2!=0)
    {
       for(i=0;i<=(n/2);i++)
       {
           c=fact(n)/(fact(n-i)*fact(i));
           printf ("%d", c);
           printf (" ");
           array_c[i]=c;
       }
        for(i=n/2;i>=0;i--)
            {printf ("%d", array_c[i]);
            printf (" ");}
    }
    else
    {
       for(i=0;i<=(n/2);i++)
       {
           c=fact(n)/(fact(n-i)*fact(i));
           printf ("%d", c);
           printf (" ");
           array_c[i]=c;
       }
        for(i=n/2-1;i>=0;i--)
           {printf ("%d", array_c[i], " ");
           printf (" ");}
    }
    return 0;
}
