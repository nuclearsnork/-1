#include <stdio.h>
#include <stdlib.h>
#include <math.h>

int main ()
{
    float a,b,x,e;
    scanf ("%f", &a);
    scanf ("%f", &b);
    scanf ("%f", &e);
    if ((a-tan(a))/(cos(a)*cos(a)*cos(a))>0) x=a;
    else x=b;
    printf("%f",x);
    while (abs((tan(x)-x)*tan(x)*tan(x))>=e)
    {
        x=x-(tan(x)-x)*tan(x)*tan(x);
    }
    printf("%f",x);
    return 0;
}
